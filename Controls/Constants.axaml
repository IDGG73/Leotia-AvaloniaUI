<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <x:Double x:Key="SingleLineHeight">24</x:Double>
    <x:Double x:Key="FieldMinimumWidth">150</x:Double>
    <x:Double x:Key="SingleLineSpacing">3</x:Double>
	<x:Double x:Key="PresenterLineHeight">20</x:Double>

	<x:Double x:Key="DefaultFontSize">13</x:Double>
	<x:Double x:Key="HeaderFontSize">15</x:Double>
	<x:Double x:Key="TitleFontSize">17</x:Double>

	<Thickness x:Key="DefaultPadding">8,4</Thickness>
    <Thickness x:Key="CardPadding">10</Thickness>
    <Thickness x:Key="SmPadding">2,1</Thickness>
    <Thickness x:Key="MdPadding">4,2</Thickness>
	
    <CornerRadius x:Key="FieldCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="DockTabCornerRadius">8,8,0,0</CornerRadius>
    <CornerRadius x:Key="SmCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="MdCornerRadius">8</CornerRadius>
    <CornerRadius x:Key="LgCornerRadius">8</CornerRadius>
    <CornerRadius x:Key="XlCornerRadius">10</CornerRadius>
    <CornerRadius x:Key="2XlCornerRadius">12</CornerRadius>
    <CornerRadius x:Key="3XlCornerRadius">14</CornerRadius>
    <CornerRadius x:Key="4XlCornerRadius">16</CornerRadius>
    <CornerRadius x:Key="FullCornerRadius">999</CornerRadius>
    <BoxShadows x:Key="Shadow">0 2 4 0 #20000000</BoxShadows>

</ResourceDictionary>